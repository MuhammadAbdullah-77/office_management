<div class="form-group">
    <table class="table table-bordered">
    <%= form_with(model: [:admins, admin], local: true) do |f| %>

    <div>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name, autofocus: true , placeholder: "First Name"%>
    </div>
    <div>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name, placeholder: "Last Name" %>
    </div>
    <div>
    <%= f.label :country%><br>
    <%= f.text_field :country, placeholder: "country" %>
    </div>
    <div>
    <%= f.label :city %><br>
    <%= f.text_field :city, placeholder: "city" %>
    </div>
    <div>
    <%= f.label :address %><br>
    <%= f.text_field :address, placeholder: "Address"%>
    </div>
    <div>
    <%= f.label :cnic %><br>
    <%= f.text_field :cnic, placeholder: "00000-0000000-0"%>
    </div>
    <div>
    <%= f.label :Phone_no %><br>
    <%= f.number_field :phone_no, placeholder: "Only digits" %>
    </div>
    <div>
    <%= f.label :Joining_date %><br>
    <%= f.text_field :joining_date, placeholder: "date" %>
    </div>
    <div>
    <%= f.label :Date_of_birth%><br>
    <%= f.text_field :dob, placeholder: "date" %>
    </div>
    <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, placeholder: "test@example.com" %>
    </div>
    <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum )</em> <% end %><br />
    <%= f.password_field :password%>
    </div>

    <div class="field">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation%>
    </div>

    <div class="actions">
    <%= f.submit "Submit", class: "btn btn-success" %>
    <%= link_to 'Back', admins_admins_path,  class: "btn btn-danger"  %>

    </div>
    <% end %>
    </table> 
</div>
